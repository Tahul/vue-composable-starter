{
  "name": "vue-composable-starter",
  "version": "1.0.0",
  "description": "ü™ù Minimalist starting point for your next Vue Composable",
  "repository": "https://github.com/Tahul/vue-composable-starter",
  "bugs": {
    "url": "https://github.com/Tahul/vue-composable-starter/issues"
  },
  "homepage": "https://github.com/Tahul/vue-composable-starter#readme",
  "keywords": [
    "vue",
    "composable"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.mjs",
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    }
  },
  "scripts": {
    "dev": "vitest dev",
    "dev:demo": "vite",
    "build": "unbuild",
    "build:demo": "vite build",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "pnpm run lint --write",
    "test": "pnpm test:types && pnpm test:unit && pnpm test:coverage",
    "test:types": "tsc --build tsconfig.json",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run",
    "dev:nuxt": "nuxi dev playground",
    "build:nuxt": "nuxi build playground",
    "start:nuxt": "nuxi preview playground"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "peerDependencies": {
    "@nuxt/kit": "^3.0.0-rc.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.19.2",
    "@types/fs-extra": "^9.0.13",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "c8": "^7.11.0",
    "eslint": "^8.12.0",
    "lint-staged": "^12.3.7",
    "nuxt": "^3.0.0-rc.8",
    "pascalcase": "^2.0.0",
    "prettier": "^2.6.1",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.2",
    "vitest": "^0.8.2",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "author": "YaeÃàl GUILLOUX <yael.guilloux@gmail.com>",
  "license": "MIT"
}
